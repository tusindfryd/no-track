{
  "name": "No track",
  "version": "1.0.0.0",
  "manifest_version": 2,
  "description": "Copy links from Facebook or Messenger without trackers.",
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}