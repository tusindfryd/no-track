{
  "name": "No track",
  "version": "2.0.0.0",
  "manifest_version": 2,
  "description": "Copy links without trackers.",
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "icons": {
    "16": "small.png",
    "48": "medium.png",
    "128": "big.png"
  }
}